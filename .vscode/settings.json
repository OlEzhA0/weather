{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.tabSize": 2,
    "typescript.tsdk": "node_modules/typescript/lib",
    "prettier.disableLanguages": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "scss"
    ],
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "prettier.eslintIntegration": true,
    "prettier.tslintIntegration": true
  }